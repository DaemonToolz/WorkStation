@model IEnumerable<WorkstationBrowser.SessionReference.DepartmentModel>

@{
    ViewBag.Title = "Index";
}

<div class="row" style="padding-top: 50px">
@foreach (var item in Model)
{
    <div class="col-md-4 col-xs-6 col-lg-3 col-sm-offset-1 col-xs-offset-1 col-md-offset-1 col-lg-offset-1" style="border: solid 1px #d7d7d7; border-radius:5px; margin-bottom:10px;">
        <div style="margin-top: 5px; vertical-align: middle;" class="panel-title">
            @Html.DisplayFor(modelItem => item.name)
            <a href="#" style="color:red" class="pull-right btn btn-default glyphicon glyphicon-remove"></a>

        </div>
        <hr />
        <a href="#" class="btn btn-default glyphicon glyphicon-cog"></a> 
        <a href="#" class="btn btn-default glyphicon glyphicon-envelope"></a> 
        <a href="#" class="btn btn-default glyphicon glyphicon-comment"></a>

        <hr />
       
        <div style="margin-bottom:10px;">
            Lorem ipsum dolor sit amet, consectetur adipiscing elit. 
            Integer dictum lorem convallis nisi imperdiet, dictum pellentesque nisi ultrices. 
            Aenean vel dignissim massa. 
     

        </div>

    </div>
}
</div>

