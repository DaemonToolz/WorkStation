@model IEnumerable<WorkstationManagementServices.Models.Database.Rank>

@{
    ViewBag.Title = "Index";
}

<h2>Index</h2>

<p>
    <a href="@Url.Action("Create")" class="btn btn-default">
        <i class="fa fa-plus" aria-hidden="true"> </i>
        Add new Rank
    </a>
</p>
<table class="table">
    <tr>
        <th>
            Rank
        </th>
        <th></th>
    </tr>

@foreach (var item in Model) {
    <tr>
        <td>
            @Html.DisplayFor(modelItem => item.name)
        </td>
        <td>
            <a href="@Url.Action("Edit", new {id = item.name})">
                <i class="glyphicon glyphicon-cog" aria-hidden="true"> </i>
            </a>
               
            <a href="@Url.Action("Details", new {id = item.name})">
                <i class="glyphicon glyphicon-zoom-in" aria-hidden="true"> </i>
            </a>
       
            <a class="del_ico" href="@Url.Action("Delete",  new {id = item.name})">
                <i class="glyphicon glyphicon-trash" aria-hidden="true"> </i>
            </a>
        </td>
    </tr>
}

</table>
